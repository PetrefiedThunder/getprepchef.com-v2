{
  "name": "@prepchef/server",
  "version": "0.1.0",
  "description": "PrepChef RegTech backend - Vendor verification as a service",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "tsx src/jobs/worker.ts",
    "worker:watch": "tsx watch src/jobs/worker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "seed:regulations": "tsx src/db/seed/seed_regulations.ts",
    "seed:expanded": "tsx src/db/seed/seed_expanded_jurisdictions.ts",
    "seed:demo": "tsx src/db/seed/seed_demo_data.ts",
    "seed:all": "npm run seed:regulations && npm run seed:expanded && npm run seed:demo",
    "db:reset": "tsx src/db/seed/reset_db.ts"
  },
  "keywords": [
    "regtech",
    "compliance",
    "food-safety",
    "vendor-verification",
    "shared-kitchen"
  ],
  "author": "PrepChef",
  "license": "UNLICENSED",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/multipart": "^8.1.0",
    "fastify": "^4.26.1",
    "mongoose": "^8.1.1",
    "bullmq": "^5.2.0",
    "ioredis": "^5.3.2",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4",
    "dotenv": "^16.4.1",
    "bcrypt": "^5.1.1",
    "axios": "^1.6.7",
    "jsonwebtoken": "^9.0.2",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@sendgrid/mail": "^8.1.0",
    "twilio": "^4.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/sendgrid__mail": "^7.4.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
